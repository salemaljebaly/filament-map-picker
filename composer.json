{
    "name": "salemaljebaly/filament-map-picker",
    "description": "A Leaflet / OpenStreetMap map picker field for Filament v5.",
    "keywords": [
        "filament",
        "laravel",
        "leaflet",
        "openstreetmap",
        "map",
        "picker"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "SalemAljebaly\\FilamentMapPicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SalemAljebaly\\FilamentMapPicker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SalemAljebaly\\FilamentMapPicker\\FilamentMapPickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
